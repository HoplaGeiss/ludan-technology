  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "deploy": "npm run build:ssr && gcloud app deploy --quiet --version v1",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "tslint --project tsconfig.json -c src/tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint --project tsconfig.json -c src/tslint.json 'src/**/*.ts' --fix",
    "e2e": "ng e2e",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run ludan-technology:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
